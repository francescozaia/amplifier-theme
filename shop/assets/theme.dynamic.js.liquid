var addToCart = {{ 'products.product.add_to_cart' | t | json }};
var soldOut = {{ 'products.product.sold_out' | t | json }};
var compareAt = {{ 'products.product.compare_at' | t | json }};
var unavailable = {{ 'products.product.unavailable' | t | json }}

theme.liquid = {
